{
  "name": "graphql-transformer-core",
  "version": "1.0.29",
  "description": "A framework to transform from GraphQL SDL to AWS CloudFormation.",
  "main": "lib/index.js",
  "scripts": {
    "test": "jest",
    "build": "tsc",
    "clean": "rm -rf ./lib",
    "prepublish": "yarn run build && cd ../.. && rm -f yarn.lock && yarn"
  },
  "keywords": [
    "graphql",
    "cloudformation",
    "aws"
  ],
  "author": "Michael Paris",
  "license": "MIT",
  "dependencies": {
    "cloudform": "^2.2.1",
    "graphql": "^0.13.2"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testURL": "http://localhost",
    "testRegex": "(src/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
