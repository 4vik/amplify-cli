{
  "name": "graphql-transformers-e2e-tests",
  "version": "1.0.29",
  "description": "End to end functional tests for appsync supported transformers.",
  "main": "lib/index.js",
  "scripts": {
    "e2e": "jest",
    "build": "tsc",
    "clean": "rm -rf ./lib"
  },
  "keywords": [
    "graphql",
    "appsync",
    "aws"
  ],
  "author": "Amazon Web Services",
  "license": "Apache-2.0",
  "dependencies": {
    "axios": "^0.18.0",
    "cloudform": "^2.2.1",
    "graphql": "^0.13.2",
    "graphql-mapping-template": "file:../graphql-mapping-template",
    "graphql-transformer-common": "file:../graphql-transformer-common",
    "graphql-transformer-core": "file:../graphql-transformer-core",
    "moment": "^2.22.2"
  },
  "devDependencies": {
    "graphql-versioned-transformer": "file:../graphql-versioned-transformer"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testURL": "http://localhost",
    "testRegex": "(src/__tests__/.*.test.*)$",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/src/__tests__/SearchableModelTransformer.e2e.test.ts"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
