{
  "name": "amplify-cli",
  "version": "0.1.0",
  "description": "amplify-cli",
  "scripts": {
    "test-changed": "lerna run test --since master",
    "test": "lerna run test",
    "pretest": "lerna run lint ",
    "e2e": "lerna run e2e",
    "lint": "lerna run lint",
    "lint-fix": "lerna run lint-fix",
    "clean": "rm -rf node_modules && lerna clean --yes && lerna run ",
    "build": "yarn wsrun --serial --exclude-missing build",
    "setup-dev": "yarn && yarn build && yarn link",
    "setup-dev-win": "yarn link",
    "publish:master": "lerna publish --canary --yes --independent  --preid=alpha --message 'chore(release): Publish [ci skip]' --no-git-tag-version --no-push",
    "publish:beta": "lerna publish --conventional-commits --cd-version=prerelease --yes --independent --npm-tag=beta --preid=beta --message 'chore(release): Publish [ci skip]'",
    "publish:release": "lerna publish --conventional-commits --cd-version=patch --yes --independent --message 'chore(release): Publish [ci skip]'",
    "postpublish:release": "git fetch . release:master && git push",
    "commit": "git-cz"
  },
  "bin": {
    "amplify": "packages/amplify-cli/bin/amplify"
  },
  "bugs": {
    "url": "https://github.com/aws-amplify/amplify-cli/issues"
  },
  "private": true,
  "homepage": "https://github.com/aws-amplify/amplify-cli#readme",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/aws-amplify/amplify-cli.git"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-push": "npm run test-changed"
    }
  },
  "author": "Amazon Web Services",
  "license": "Apache-2.0",
  "dependencies": {
    "@aws-amplify/cli": "file:packages/amplify-cli",
    "amplify-category-analytics": "file:packages/amplify-category-analytics",
    "amplify-category-api": "file:packages/amplify-category-api",
    "amplify-category-auth": "file:packages/amplify-category-auth",
    "amplify-category-function": "file:packages/amplify-category-function",
    "amplify-category-hosting": "file:packages/amplify-category-hosting",
    "amplify-category-interactions": "file:packages/amplify-category-interactions",
    "amplify-category-notifications": "file:packages/amplify-category-notifications",
    "amplify-category-storage": "file:packages/amplify-category-storage",
    "amplify-codegen": "file:packages/amplify-codegen",
    "amplify-frontend-android": "file:packages/amplify-frontend-android",
    "amplify-frontend-ios": "file:packages/amplify-frontend-ios",
    "amplify-frontend-javascript": "file:packages/amplify-frontend-javascript",
    "amplify-graphql-docs-generator": "file:packages/amplify-graphql-docs-generator",
    "amplify-provider-awscloudformation": "file:packages/amplify-provider-awscloudformation",
    "graphql-http-transformer": "file:packages/graphql-http-transformer",
    "graphql-mapping-template": "file:packages/graphql-mapping-template",
    "graphql-transformers-e2e-tests": "file:packages/graphql-transformers-e2e-tests"
  },
  "devDependencies": {
    "@commitlint/cli": "^7.1.2",
    "@commitlint/config-conventional": "^7.1.2",
    "@commitlint/config-lerna-scopes": "^7.1.2",
    "@types/axios": "^0.14.0",
    "@types/ejs": "^2.6.0",
    "@types/graphql": "^0.13.1",
    "@types/handlebars": "^4.0.39",
    "@types/jest": "^23.1.1",
    "@types/node": "^10.3.4",
    "@types/prettier": "^1.13.2",
    "ava": "^0.22.0",
    "aws-amplify": "^1.1.4",
    "aws-sdk": "^2.259.1",
    "babel-eslint": "^8.0.1",
    "bestzip": "^2.0.0",
    "codecov": "^3.0.0",
    "commitizen": "^3.0.2",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.12.0",
    "graphql-appsync-transformer": "file:packages/graphql-appsync-transformer",
    "graphql-auth-transformer": "file:packages/graphql-auth-transformer",
    "graphql-connection-transformer": "file:packages/graphql-connection-transformer",
    "graphql-dynamodb-transformer": "file:packages/graphql-dynamodb-transformer",
    "graphql-elasticsearch-transformer": "file:packages/graphql-elasticsearch-transformer",
    "graphql-transformer-common": "file:packages/graphql-transformer-common",
    "graphql-transformer-core": "file:packages/graphql-transformer-core",
    "graphql-versioned-transformer": "file:packages/graphql-versioned-transformer",
    "husky": "^1.1.0",
    "jest": "^23.1.0",
    "lerna": "^3.4.3",
    "mockirer": "^2.0.1",
    "node-fetch": "^2.2.0",
    "nyc": "^11.2.1",
    "prettier": "^1.14.2",
    "ts-jest": "^22.4.6",
    "tslint": "^5.10.0",
    "typescript": "^2.8.3",
    "wsrun": "^3.3.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "workspaces": [
    "packages/*"
  ]
}
